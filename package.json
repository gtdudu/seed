{
  "name": "stack",
  "version": "1.0.0",
  "description": "Mighty",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/serverProd.js",
    "dev": "npm run clean ; cross-env NODE_ENV=development nodemon --watch server server/serverDev --exec babel-node --presets es2015,stage-2",
    "test": "cross-env NODE_ENV=test karma start karma.conf.js",
    "test:server": "cross-env NODE_ENV=test nyc mocha ./chai.js ./server --recursive",
    "prod": "npm run build ; npm start",
    "build": "npm run clean ; npm run build:client; npm run build:server",
    "build:client": "webpack --config config-webpack.prod.client",
    "build:server": "cross-env BABEL_ENV=production babel server/ -d dist --presets es2015,stage-2;",
    "lint": "eslint ./client ./server ",
    "lint:fix": "eslint ./client ./server --fix",
    "clean": "rimraf coverage dist .nyc_output"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "include": [
      "server/**/*.js",
      "client/**/*.js"
    ],
    "exclude": [
      "client/index.js",
      "server/serverDev.js",
      "server/serverProd.js",
      "server/app.js"
    ],
    "report-dir": "./coverage/server",
    "sourceMap": false,
    "instrument": false,
    "check-coverage": true,
    "per-file": false,
    "lines": 80,
    "statements": 80,
    "functions": 80,
    "branches": 80,
    "cache": true
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.2",
    "cross-env": "^5.1.1",
    "express": "^4.16.2",
    "express-http-proxy": "^1.0.7",
    "joy": "^0.1.1",
    "lodash": "^4.17.4",
    "morgan": "^1.9.0"
  },
  "devDependencies": {
    "@uirouter/angularjs": "^1.0.10",
    "angular": "^1.6.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^3.5.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^3.0.0",
    "chai-things": "^0.2.0",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.2.0",
    "css-loader": "^0.28.7",
    "dirty-chai": "^1.2.0",
    "eslint": "^4.10.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-angular": "^3.1.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-loader": "^0.5.1",
    "ignore-styles": "^5.0.1",
    "jsdom": "11.3.0",
    "jsdom-global": "3.0.2",
    "karma": "^1.7.1",
    "karma-babel-preprocessor": "^7.0.0",
    "karma-coverage": "^1.1.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-dirty-chai": "^1.0.2",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sinon-chai": "^1.3.3",
    "karma-spec-reporter": "0.0.31",
    "mocha": "^4.0.1",
    "node-sass": "^4.6.0",
    "nodemon": "^1.12.1",
    "nyc": "^11.3.0",
    "postcss-loader": "^2.0.8",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "sinon": "^4.1.2",
    "sinon-chai": "^2.14.0",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-node-externals": "^1.6.0"
  },
  "keywords": [
    "es6",
    "babel",
    "express",
    "angular",
    "webpack",
    "nodemon",
    "eslint",
    "nyc",
    "karma",
    "mocha",
    "sinon",
    "chai",
    "istanbul"
  ]
}
